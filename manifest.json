{
  "name": "Chrome导航",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Chrome导航,干掉百度首页!",
  "browser_action": {
    "default_icon": "start_19.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "https://www.baidu.com/*"
  ],
  "icons": {
      "128": "start_128.png",
      "16": "start_16.png",
      "48": "start_48.png"
   }
}